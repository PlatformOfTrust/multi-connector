{
  "plugins": [
      "oauth2",
      "kone-elevator-call"
  ],
  "protocol": "websocket",
  "authConfig": {
    "url": "${url}",
    "authUrl" : "${authUrl}",
    "authPath" : "${authPath}",
    "username": "${username}",
    "password": "${password}",
    "clientId": "${clientId}",
    "clientSecret": "${clientSecret}",
    "clientAuth": "${clientAuth}",
    "grantType": "${grantType}",
    "scope": "${scope}",
    "path": "${id}",
    "event": "${event}"
  },
  "dataObjects": [
    ""
  ],
  "dataPropertyMappings": {
    "Status": "data.success",
    "Error": "data.error",
    "Message": "data.message"
  },
  "generalConfig": {
    "hardwareId": {
      "dataObjectProperty": "id"
    },
    "timestamp": {
      "dataObjectProperty": "timestamp"
    }
  },
  "output": {
    "array": "sensors",
    "data": "measurements"
  }
}
