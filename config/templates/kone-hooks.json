{
    "plugins": [
        "oauth2",
        "kone-elevator-movement",
        "streamer"
    ],
    "protocol": "hook",
    "authConfig": {
        "url": "${url}",
        "authPath": "${authPath}",
        "clientId": "${clientId}",
        "clientSecret": "${clientSecret}",
        "clientAuth": "${clientAuth}",
        "grantType": "${grantType}",
        "scope": "${scope}",
        "path": "${id}"
    },
    "dataObjects": [
        ""
    ],
    "dataPropertyMappings": {
        "MeasureOccupancy": "value"
    },
    "generalConfig": {
        "hardwareId": {
            "dataObjectProperty": "id"
        },
        "timestamp": {
            "dataObjectProperty": "time"
        }
    },
    "output": {
        "array": "sensors",
        "data": "measurements"
    }
}
