{
    "plugins": [
        "basic",
        "hydrolink"
    ],
    "protocol": "rest",
    "authConfig": {
        "url": "${url}",
        "username": "${username}",
        "password": "${password}",
        "path": "/api/${idLocal}/${start}/${end}/json"
    },
    "dataObjects": [
        ""
    ],
    "dataPropertyMappings": {
        "${type}": ""
    },
    "generalConfig": {
        "timestamp": {
            "dataObjectProperty": "timestamp"
        }
    },
    "input": {
        "required": [
            "parameters.targetObject",
            "parameters.period"
        ]
    },
    "output": {
        "contextValue": "https://standards.oftrust.net/v2/Context/DataProductOutput/Process/Measure/WaterMeterReading/",
        "array": "process"
    }
}
