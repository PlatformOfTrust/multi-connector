{
  "template": "granlund-manager",
  "static": {
    "url": "<url>",
    "username": "<username>",
    "password": "<password>",
    "clientId": "<client-id>",
    "clientSecret": "<client-secret>",
    "type": "Case",
    "headers": {
      "Accept-Language": "fi",
      "X-CustomerId": "<customer-id>"
    }
  },
  "dynamic": {
    "authConfig.path": "targetObject.idLocal"
  },
  "request": {
    "targetObject": {
      "idLocal": "<object-id>"
    }
  }
}
