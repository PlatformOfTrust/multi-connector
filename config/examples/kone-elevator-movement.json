{
  "template": "kone-hooks",
  "static": {
    "url": "https://dev.kone.com",
    "authPath" : "/api/v2/oauth2/token",
    "clientAuth": "header",
    "clientId": "<client-id>",
    "clientSecret": "<client-secret>",
    "grantType": "client_credentials",
    "scope": "application/inventory callgiving/* equipmentstatus/* application/subscriptions"
  },
  "dynamic": {
    "authConfig.path": "ids"
  },
  "request": {
    "ids": [
      "<source-id>"
    ]
  }
}
