{
  "template": "rpc-ws-generic",
  "static": {
    "url": "wss://<ip-address>/eg",
    "event": ["AttributeChangeAlert", "Occurrence", "Recurrence"],
    "username": "<username>",
    "password": "<password>",
    "type": "MeasurePresence"
  },
  "dynamic": {
    "authConfig.path": "ids"
  },
  "plugins": {
    "streamer": {
      "url": "<storage-url>"
    }
  },
  "request": {
    "ids": [
      "<id>"
    ]
  }
}
