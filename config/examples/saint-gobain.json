{
    "template": "saint-gobain",
    "static": {
        "url": "https://api.saint-gobain.dk",
        "path": "/service-call/<service-call>",
        "username": "<username>",
        "password": "<password>",
        "clientId": "<client-id>",
        "clientSecret": "<client-secret>",
        "type": "ProductCatalog"
    },
    "plugins": {
        "millis": {
            "format": "seconds"
        }
    },
    "schema": "product-catalog-v4.1_saint-gobain",
    "dynamic": {
        "authConfig.path": "period"
    },
    "request": {
        "period": "<period>"
    }
}
